    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.5.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "quick-lint-js": "^3.2.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
